{
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "@shared": "./src/app/shared",
          "@introspection": "./src/app/core/api-introspection",
          "@core": "./src/app/core",
          "~": "./src/app"
        }
      }
    ]
  ]
}
